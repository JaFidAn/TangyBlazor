<div class="row">
	<h4 class="text-primary pl-4 pt-2">Grand Child Component</h4>
	<p>There is a message - @MessageForGrandChild</p>
	<p>Lucky Number  - @LuckyNumberFromParent</p>
</div>
@code {
	[CascadingParameter(Name = "message")]
	public string MessageForGrandChild { get; set; }

	[CascadingParameter(Name = "luckyNumber")]
	public string LuckyNumberFromParent { get; set; }
}
